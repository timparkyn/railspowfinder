<p id="notice"><%= notice %></p>

<h1>Stations</h1>

<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Location name</th>
      <th colspan="3"></th>
    </tr>
  </thead>




  <tbody>
    <% @stations.each do |station| %>
      <tr>
        <td><%= station.code %></td>
        <td><%= station.description %></td>
        </tr>
    <% end %>
  </tbody>
</table>



<%= render 'weatherDisplay' %>

